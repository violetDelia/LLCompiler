function(build_symengine lib_path)
    message("******************************************")
    message("************* build symengine ***************")
    message("******************************************")
    set(WITH_SYMENGINE_ASSERT yes)
    set(WITH_SYMENGINE_RCP yes)
    set(WITH_SYMENGINE_THREAD_SAFE res)
    set(WITH_ECM no)
    set(WITH_PRIMESIEVE no)
    set(WITH_FLINT no)
    set(WITH_ARB no)
    set(WITH_TCMALLOC no)
    set(WITH_PIRANHA no)
    set(WITH_MPFR no)
    set(WITH_MPC no)
    set(WITH_LLVM no)
    set(WITH_OPENMP no)
    set(WITH_SYSTEM_CEREAL no)
    set(BUILD_TESTS no)
    add_subdirectory(${lib_path})
endfunction(build_symengine)